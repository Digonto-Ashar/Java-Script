var names = ["ashar", "digonto", "akash", "fahim", "moin"];
document.write(names[3]+"<br>"+ names.length);

names.push("abir");
document.write("<br>"+names+"<br>"+ names.length+"<br>");

//loop in array

var num = [];

for(var i =0; i <5; i++){
    num[i]= parseInt(prompt("Enter number : "));

}

var sum= 0;

for( i= 0; i<5;i++){
    document.write(num[i]+"<br>");
    sum = sum+num[i];
}

document.write("Sum = "+sum)
